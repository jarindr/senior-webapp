<!DOCTYPE html>
<html>
<% include head-tag %>
<style>
.footer{
  position: fixed;
  bottom: 0;
  width: 100%;
}
</style>
<body>
  <div class = "app">
    <div class='stat-component'>
      <%include _navbar%>
      <div class='stat-content'>
        <div class='controller'><i class="fa fa-bars" aria-hidden="true"></i></div>
        <div class='row' style="padding-left:3%;">

          <!-- start controller display -->
          <div class='col s3 control-display'>
            <div class='general-number-graph-header'>
              Percentage of
              <!-- <br><span class='type-selected-graph'></span><br> -->
              in faculties 2015
            </div>

            <select class="browser-default select-query-honor">
            </select>

            <form action="#" style="float:right;">
              <p>
                <input class='with-gap' name="group1" type="radio" id="fixed" checked/>
                <label for="fixed" style='margin-right:10px;'>Fixed</label>
                <input class='with-gap' name="group1" type="radio" id="asc"/>
                <label for="asc" style='margin-right:10px;'>Ascendent</label>
                <input class='with-gap' name="group1" type="radio" id="dsc" />
                <label for="dsc">Descendent</label>
              </p>
              <button type='button' class='btn' id="controller" style="padding-right:20px; width:100%;">
                Categories <i class="fa fa-caret-down" aria-hidden="true"></i>
              </button>
              <div class='checkbox-list-container'>
                <ul>
                </ul>
              </div>
            </form>
          </div>
          <!-- end controller display -->

          <div class='col s9' id="general-number-graph-container">
            <div id='general-number-graph'>
            </div>
          </div>
        </div>
      </div>
      <%include _footer%>
    </div>
  </div>
</div>

<!-- start vendors -->
<script type="text/javascript" src="../public/javascripts/graphs/circle.js" charset="utf-8"></script>
<script type="text/javascript" src="../bower_components/jquery/dist/jquery.js" charset="utf-8"></script>
<script type="text/javascript" src="../bower_components/materialize/dist/js/materialize.js" charset="utf-8"></script>

<script src="./bower_components/highcharts/highstock.js"></script>
<!-- end vendors -->
<script src="./bower_components/highcharts/modules/drilldown.js"></script>

<script type="text/javascript" src="./build/js/bundle.js" charset="utf-8"></script>


</body>
</html>
